n      \n      return item;\n    });\n    // console.log(items);\n    putItems(newItems);\n    // console.log(items);\n  };\n  const handleAdd = text => {\n    console.log(\"Adding\");\n    console.log(text);\n    putItems([...items, { key: getKey(), text, done: false }]);\n  };\n  return (\n    <div className=\"panel\">\n      <div className=\"panel-heading\">\n        ITSS ToDoアプリ\n      </div>\n      <Input onAdd={handleAdd} />\n      <Filter\n        onChange={handleFilterChange}\n        value={filter}\n      />\n      {displayItems.map(item => (\n        <TodoItem \n          key={item.key}\n          item={item}\n          onCheck={handleCheck}\n        />\n      ))}\n      <div className=\"panel-block\">\n        {items.length} items\n      </div>\n      <div className=\"panel-block\">\n        <button \n          className=\"button is-light is-fullwidth\"                         onClick=  {clearItems}\n        >\n          全てのToDoを削除\n        </button>\n      </div>\n    </div>\n  );\n}\n\nexport default Todo;","/home/runner/2022ITSSReactTodoK63Fork1-1/src/components/Input.js",["45"],"import React, { useState } from 'react';\n\nfunction Input( { onAdd } ) {\n  const [text, setText] = React.useState('');\n\n  const handleChange = e => setText(e.target.value);\n\n  const handleKeyPress = e => {\n    if (e.keyCode === 13) {\n      onAdd(text);\n      setText('');\n    }\n  };\n\n  return (\n    <div className=\"panel-block\">\n      <input\n        class=\"input\"\n        type=\"text\"\n        placeholder=\"Todoを入力してください\"\n        value={text}\n        onChange={handleChange}\n        onKeyDown={handleKeyPress}\n      />\n    </div>\n  );\n}\n\nexport default Input;","/home/runner/2022ITSSReactTodoK63Fork1-1/src/components/TodoItem.js",[],"/home/runner/2022ITSSReactTodoK63Fork1-1/src/components/Filter.js",["46","47","48"],"/home/runner/2022ITSSReactTodoK63Fork1-1/src/hooks/storage.js",[],"/home/runner/2022ITSSReactTodoK63Fork1-1/src/lib/util.js",[],{"ruleId